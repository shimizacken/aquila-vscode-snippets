  "Test it.todo": {
    "prefix": ["itt"],
    "body": ["it.todo('should $1, when $2');"],
    "description": "Jest it with todo"
  },
  "Test it": {
    "prefix": ["it"],
    "body": ["it('should $1, when $2', () => {", "\t$3", "});"],
    "description": "Jest it"
  },
  "Test it async": {
    "prefix": ["ita"],
    "body": ["it('should $1, when $2', async () => {", "\t$3", "});"],
    "description": "Jest it"
  },
